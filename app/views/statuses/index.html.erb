<p id="notice"><%= notice %></p>

<h1>Statuses</h1>

<table>
  <thead>
    <tr>
      <th style="text-align:right">#ID</th>
      <th style="text-align:center">Type</th>
      <th>Timestamp</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @statuses.each do |status| %>
      <tr>
        <td><%= status.object_id %></td>
        <td><%= status.object_type %></td>
        <td><%= status.timestamp.to_formatted_s(:long) %></td>
        <td><%= status.state %></td>
        <td><%= link_to 'Show', status %></td>
        <!-- <td><%= link_to 'Edit', edit_status_path(status) %></td> -->
        <!-- <td><%= link_to 'Destroy', status, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Status', new_status_path %>

<%= link_to 'Query Status', query_statuses_path %>

<%= link_to 'Upload CSV', upload_statuses_path %>
